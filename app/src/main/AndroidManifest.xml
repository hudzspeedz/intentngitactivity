<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coventery.jack.activityopen">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="@string/title_activity_second"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ThirdActivity"
            android:label="@string/title_activity_third"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WebActivity"
            android:label="@string/title_activity_web"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Fragtivity"
            android:label="@string/title_activity_fragtivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Rotatefrag"
            android:label="@string/title_activity_rotatefrag"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Layouts"
            android:label="@string/title_activity_layouts"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyWidgets"
            android:label="@string/title_activity_my_widgets"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProgressBarActivity"
            android:label="@string/title_activity_progress_bar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainListActivity"
            android:label="@string/title_activity_main_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
